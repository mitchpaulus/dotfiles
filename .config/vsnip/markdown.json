{
	"link": {
		"prefix": ["li"],
		"body": [
		   "[$1]($2)$0"
		],
		"description": "Inserts a link"
	},
    "fraction": {
        "prefix": ["fr"],
        "body": [
            "\\frac{$1}{$2}$0"
        ],
        "description": "Fraction in Tex Equations"
    },
    "lr": {
        "prefix": ["lr"],
        "body": [
            "\\left($1\\right)$0"
        ],
        "description": "Left and Right in Tex Equations"
    },
    "rm": {
        "prefix": ["rm"],
        "body": [
            "\\textrm{$1}$0"
        ],
        "description": "rm in Tex Equations"
    },
    "dot": {
        "prefix": ["dot"],
        "body": [
            "\\dot{$1}$0"
        ],
        "description": "Dot in Tex Equations"
    },
    "histogram": {
        "prefix": ["hist"],
        "body": [
            "<!-- Compass: Histogram -->",
            "Scope: $1",
            "Label: $2",
            "IgnoreLabels: $3",
            "<!-- Compass -->"
        ],
        "description": "Compass Histogram"
    },
    "default": {
        "prefix": ["default"],
        "body": [
            "<!-- Compass: Default -->",
            "Scope: $1",
            "StartDate: $2",
            "EndDate: $3",
            "MinuteInterval: $4",
            "<!-- Compass -->"
        ],
        "description": "Compass Default"
    },
    "StartDate": {
        "prefix": ["sd"],
        "body": [
            "StartDate: $1"
        ],
        "description": "Compass StartDate"
    },
    "LabelStatisticsTable": {
        "prefix": ["lst", "ls", "stats"],
        "body": [
            "<!-- Compass: LabelStatisticsTable -->",
            "Scope: $1",
            "Label: $2",
            "IgnoreLabels: $3",
            "<!-- Compass -->"
        ],
        "description": "Compass LabelStatisticsTable"
    },
    "Image": {
        "prefix": ["img"],
        "body": ["![$1]($2)"],
        "description": "Markdown Image"
    },
    "Equation": {
        "prefix": ["eq"],
        "body": ["\\$\\$", "$1", "\\$\\$" ],
        "description": "Markdown Equation"
    },
    "Inline Equation": {
        "prefix": ["ie"],
        "body": ["\\$$1\\$"],
        "description": "Markdown Inline Equation"
    },
    "align": {
        "prefix": ["al"],
        "body": [
            "\\begin{align*}",
            "$1",
            "\\end{align*}"
        ],
        "description": "Align in Tex Equations"
    },
    "figure": {
        "prefix": ["fig"],
        "body": [ "![$1]($2)" ],
        "description": "Figure in Tex Equations"
    }
}
