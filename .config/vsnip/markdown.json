{
	"link": {
		"prefix": ["li"],
		"body": [
		   "[$1]($2)$0"
		],
		"description": "Inserts a link"
	},
    "fraction": {
        "prefix": ["fr"],
        "body": [
            "\\frac{$1}{$2}$0"
        ],
        "description": "Fraction in Tex Equations"
    },
    "lr": {
        "prefix": ["lr"],
        "body": [
            "\\left($1\\right)$0"
        ],
        "description": "Left and Right in Tex Equations"
    },
    "dot": {
        "prefix": ["dot"],
        "body": [
            "\\dot{$1}$0"
        ],
        "description": "Dot in Tex Equations"
    },
    "histogram": {
        "prefix": ["hist"],
        "body": [
            "<!-- Compass: Histogram -->",
            "Scope: $1",
            "Label: $2",
            "IgnoreLabels: $3",
            "<!-- Compass -->"
        ],
        "description": "Compass Histogram"
    },
    "default": {
        "prefix": ["default"],
        "body": [
            "<!-- Compass: Default -->",
            "Scope: $1",
            "StartDate: $2",
            "EndDate: $3",
            "MinuteInterval: $4",
            "<!-- Compass -->"
        ],
        "description": "Compass Default"
    },
    "StartDate": {
        "prefix": ["sd"],
        "body": [
            "StartDate: $1"
        ],
        "description": "Compass StartDate"
    },
    "LabelStatisticsTable": {
        "prefix": ["lst", "ls", "stats"],
        "body": [
            "<!-- Compass: LabelStatisticsTable -->",
            "Scope: $1",
            "Label: $2",
            "IgnoreLabels: $3",
            "<!-- Compass -->"
        ],
        "description": "Compass LabelStatisticsTable"
    },
    "Image": {
        "prefix": ["img"],
        "body": ["![$1]($2)"],
        "description": "Markdown Image"
    }
}
