{
    "function":
    {
        "prefix": "fn",
        "body": [
            "function ${1:name}",
            "  $0",
            "end"
        ],
        "description": "function"
    },
    "last":
    {
        "prefix": "last",
        "body": [
            "string sub -e -${1:num} ${2:string}"
        ],
        "description": "trim last"
    },
    "if command":
    {
        "prefix": "ic",
        "body": [
            "if command ${1:command} >/dev/null 2>&1",
            "    $0",
            "end"
        ],
        "description": "if command exists"
    }
}
