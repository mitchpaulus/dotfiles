{
    "p": {
        "prefix": "p",
        "body": [
            "<p>",
            "$1",
            "</p>"
        ],
        "description": "p"
    },
    "frac": {
        "prefix": "frac",
        "body": [
            "\\frac{$1}{$2}"
        ],
        "description": "frac"
    },
    "dot": {
        "prefix": "dot",
        "body": [
            "\\dot{$1}"
        ],
        "description": "dot"
    },
    "left-right": {
        "prefix": "left-right",
        "body": [
            "\\left($1\\right)"
        ],
        "description": "left-right"
    },
    "unordered-list": {
        "prefix": "ul",
        "body": [
            "<ul>",
            "  <li>$1</li>",
            "</ul>"
        ],
        "description": "unordered-list"
    },
    "ordered-list": {
        "prefix": "ol",
        "body": [
            "<ol>",
            "  <li>$1</li>",
            "</ol>"
        ],
        "description": "ordered-list"
    },
    "list-item": {
        "prefix": "li",
        "body": [
            "<li>$1</li>"
        ],
        "description": "list-item"
    },
    "html": {
        "prefix": "html",
        "body": [
            "<!DOCTYPE html>",
            "<html>",
            "<head>",
            "  <title>$1</title>",
            "</head>",
            "<body>",
            "  $2",
            "</body>",
            "</html>"
        ],
        "description": "html"
    },
    "div": {
        "prefix": "div",
        "body": [
            "<div>",
            "  $1",
            "</div>"
        ],
        "description": "div"
    },
    "tr": {
        "prefix": "tr",
        "body": [ "<tr> $1 </tr>" ],
        "description": "tr"
    },
    "td": {
        "prefix": "td",
        "body": [ "<td> $1 </td>" ],
        "description": "td"
    },
    "th": {
        "prefix": "th",
        "body": [ "<th> $1 </th>$0" ],
        "description": "th"
    },
    "table": {
        "prefix": "table",
        "body": [
            "<table>",
            "$1",
            "</table>"
        ],
        "description": "table"
    },
    "link": {
        "prefix": "a",
        "body": [
            "<a href=\"$1\">$2</a>"
        ],
        "description": "link"
    },
    "h1": {
        "prefix": "h1",
        "body": [
            "<h1>$1</h1>"
        ],
        "description": "h1"
    },
    "h2": {
        "prefix": "h2",
        "body": [
            "<h2>$1</h2>"
        ],
        "description": "h2"
    },
    "h3": {
        "prefix": "h3",
        "body": [
            "<h3>$1</h3>"
        ],
        "description": "h3"
    },
    "code": {
        "prefix": "c",
        "body": [
            "<code>$1</code>"
        ],
        "description": "code"
    },
    "italic": {
        "prefix": "i",
        "body": [
            "<i>$1</i>"
        ],
        "description": "italic"
    }
}
