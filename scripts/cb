#!/usr/bin/env mshell

args (a! @a '-h' = @a '--help' = or) any if
'cb - Put clipboard contents to standard output

USAGE: cb' wl
0 exit
end

$WSL_DISTRO_NAME? "windows" runtime = or if
     ['powershell.exe' 'Get-Clipboard'] * !
      "\r" "" findReplace
      # '\n$' '' reReplace
      w
else
     [xclip -o -selection clipboard] * ! w
end
