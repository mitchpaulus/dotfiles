#!/usr/bin/env mshell

# 'gg' => goto github
tempDir `gh_repos.txt` / repoFile!

@repoFile isFile not (@repoFile readFile lines len 2 <) or if
    $"Rebuilding gh_repos.txt.." wle
    [github_repos_msh] @repoFile > !
    # $"{@repoFile} not found. Usually made using\ngithub_repos > {@repoFile}\nor\n[github_repos] `{@repoFile}` >;" wle 1 exit
end

[fzf] @repoFile toPath < *! trim repo!
[wsl_url_open $"https://github.com/{@repo}"]!
