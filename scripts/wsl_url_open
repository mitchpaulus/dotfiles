#!/usr/bin/env mshell
soe
$1 "https://" startsWith not ($"https://{$1}") ($1) iff url!

# Check if we are in WSL or not.
$WSL_DISTRO_NAME?
(
    [wslpath -u [winenv USERPROFILE] os;]os; cd
    [cmd.exe /C start $1];
)
([xdg-open @url];)
iff

