#!/bin/sh

# Autocorrect Update
if test -e ~/.local/share/nvim/plugged/autocorrect.vim/updatecorrections.sh; then
    cd ~/.local/share/nvim/plugged/autocorrect.vim
    ./updatecorrections.sh
elif test -e ~/.config/nvim/plugged/autocorrect.vim/updatecorrections.sh; then
    cd ~/.config/nvim/plugged/autocorrect.vim
    ./updatecorrections.sh
else
    printf "Did not find autocorrect.vim\n"
fi
